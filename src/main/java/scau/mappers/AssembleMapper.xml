<mapper namespace="scau.mappers.AssembleMapper">

<select id="customizedQuery" parameterType="map" resultType="java.util.LinkedHashMap" statementType="STATEMENT">
select ${x},${y} from ${t} group by ${x}
</select>

<select id="boxPlotQuery" parameterType="map" resultType="java.util.LinkedHashMap" statementType="STATEMENT">
select ${x},${y} from ${t}
</select>

<select id="lineGraphQuery" parameterType="map" resultType="java.util.LinkedHashMap" statementType="STATEMENT">
select ${x} from ${t} group by ${x}
</select>


</mapper>